<mvc:View
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
    <List
        headerText="{i18n>invoiceListTitle}"
        class="sapUiResponsiveMargin"
        width="auto"
        items="{invoices>/Invoices}">
            <items>
                <ObjectListItem 
                    title="{invoices>Quantity} x {invoices>ProductName}"/>
            </items>
    </List>

</mvc:View>